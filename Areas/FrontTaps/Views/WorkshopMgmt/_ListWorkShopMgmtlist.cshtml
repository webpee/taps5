<table class="table table-striped table-bordered table-condensed bootstrap-datatable datatable">
    <thead>
        <tr>
            <th style="width:3%;">No</th>
            <th>Image</th>
            <th>WorkShopName</th>
            @*<th>PersonInCharge</th>*@
            <th>Contact No.</th>
            @*<th style="width:10%">Email</th>*@
            <th>Address</th>
            <th>Country</th>
            <th>State</th>
            <th>City</th>
            <th>Verified</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="o in filtered = (list1| filter:searchFilter1:strict) | startFrom:(currentPage-1)*entryLimit | limitTo:entryLimit">
            <td> {{$index+1}} </td>
            <td>
                <img ng-src="~/MediaUploads/{{o.ImageUrl}}" err-src="~/assets/img/no_image.jpg" class="img-polaroid" ng-model="model.ImageUrl" style="height:60px;width:60px;" alt="No Image" />
            </td>
            <td> {{o.Name}}  </td>
            @*<td>{{o.PersonInCharge}} </td>*@
            <td>{{o.ContactNumber}} </td>
            @*<td>{{o.Email}} </td>*@
            <td>{{o.Address}} </td>
            <td>{{o.CountryName}} </td>
            <td>{{o.StateName}} </td>
            <td>{{o.CityName}} </td>
            <td>{{o.Verified}} </td>

            <td class="center">
                <a href="#" class="btn btn-info" ng-click="FetchWorkshopById(o)" ng-show="!o.LockStatus" title="Edit" data-rel="tooltip">
                    <i class="fa fa-edit"></i>
                </a>
                <a href="#" class="btn btn-danger" ng-really-message="Are you sure to Delete this Record ?" ng-really-click="DeleteWorkshop(o)" title="Delete" data-rel="tooltip">
                    <i class="fa fa-trash-o"></i>
                </a>
            </td>
        </tr>
    </tbody>
</table>
<div class="row show-hide-animation">
    <div class="col-md-12">
        <pagination page="currentPage" class="pull-right" max-size="noOfPages" total-items="totalItems" items-per-page="entryLimit" data-boundary-links="true"></pagination>
    </div>
</div>






