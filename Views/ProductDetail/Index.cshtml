@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Areas/FrontTaps/Css/jquery.raty.css" rel="stylesheet" />
<link href="~/Content/slick.css" rel="stylesheet" />
