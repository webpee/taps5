<table class="table table-striped table-bordered table-condensed bootstrap-datatable datatable">
    <thead>
        <tr>
            <th style="width:5%">No</th>
            <th style="width:19%">Model</th>
            <th style="width:19%">Trim</th>
            <th style="width:19%">Engine</th>
            <th style="width:19%">Transmission</th>
            <th style="width:8%">Year</th>
            <th style="width:11%">Action</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="o in filtered = (list1| filter:searchFilter1:strict) | startFrom:(currentPage-1)*entryLimit | limitTo:entryLimit">
            <td>
                {{$index+1}}
            </td>
            <td>
                {{o.ModelName}}
            </td>
            <td>
                {{o.TrimName}}
            </td>
            <td>
                {{o.EngineName}}
            </td>
            <td>
                {{o.TransmissionName}}
            </td>
            <td>
                {{o.Year}}
            </td>

            <td class="center">
                <a href="#" class="btn btn-info" ng-click="FetchModelYearById(o)" ng-show="!o.LockStatus" title="Edit" data-rel="tooltip">
                    <i class="fa fa-edit"></i>
                </a>
                <a href="#" class="btn btn-danger" ng-really-message="Are you sure to Delete this Record ?" ng-really-click="DeleteModelYear(o)" title="Delete" data-rel="tooltip">
                    <i class="fa fa-trash-o"></i>
                </a>
            </td>
        </tr>
    </tbody>
</table>
<div class="row show-hide-animation">
    <div class="col-md-12">
        <pagination page="currentPage" class="pull-right" max-size="noOfPages" total-items="totalItems" items-per-page="entryLimit" data-boundary-links="true"></pagination>
    </div>
</div>






